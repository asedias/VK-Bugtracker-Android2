        <solid android:color="#ff426c9f" />
        <corners android:radius="1000dp" />
    </shape>
</item>
<item android:state_enabled="true">
    <shape>
        <solid android:color="@color/colorPrimary" />
        <corners android:radius="1000dp" />
    </shape>
</item>
<item>
    <shape>
        <solid android:color="#665181b8" />
        <corners android:radius="1000dp" />
    </shape>
</item>
</selector>